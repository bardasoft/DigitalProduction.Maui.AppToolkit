	xmlns:dpcontrols="clr-namespace:DigitalProduction.Controls;assembly=DigitalProductionMauiLibrary"
	xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit">
	<!--<Style TargetType="{x:Type dpcontrols:HyperlinkSpan}">
		<Setter Property="TextColor" Value="{DynamicResource White}"/>
	</Style>-->

	<Style TargetType="{x:Type dpcontrols:HyperlinkLabel}" x:Key="HyperLinkStyle" Class="HyperLinkStyle">
